{
  "manifest_version": 2,
  "name": "Dark Google Calendar",
  "version": "1.0.1",

  "description": "Turns Google Calendar into a dark theme",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },

  "author": "alberto_rota@outlook.com",
  "content_scripts": [
    {
      "matches": ["https://calendar.google.com/*"],
    //   "js": ["js/content.js"],
      "css": ["css/content.css"]
    }
  ]
}